<h3>Coupon Form</h3>

<%= form_for(@coupon) do |f| %>
  <%= f.label :coupon_code %>
  <%= f.text_field :coupon_code%>
  <%= f.label :store %>
  <%= f.text_field :store%>
  <%= f.submit value="Submit Coupon"%>
<% end %>

